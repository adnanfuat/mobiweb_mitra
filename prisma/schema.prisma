  keywords          String?  @db.Text
  title             String?  @db.Text
  alternatif_sozler String?  @db.Text
  vitrin            String?  @db.Char(1)
  aciklama          String   @db.Text
  htaccess_etiket   String?  @db.VarChar(100)
  altkategori_no    Int      @default(0) @db.UnsignedInt
  aciklama_tarihi   DateTime @db.DateTime(0)
  keywords_exclude  String?  @db.Text

  @@index([ustkategori_no], map: "FK_altsektor_siniflandirma_altsektorler")
  @@fulltext([ad], map: "ad")
}

model altsektorler {
  numara                Int        @id @default(autoincrement()) @db.UnsignedInt
  ustkategori_no        Int        @db.UnsignedInt
  vitrin                String?    @db.Char(1)
  ad                    String     @default("") @db.VarChar(100)
  resim_adres           String?    @db.Text
  title                 String?    @db.Text
  description           String?    @db.Text
  keywords_include      String?    @db.Text
  keywords_exclude      String?    @db.Text
